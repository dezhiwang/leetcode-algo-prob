class Solution:
    def getRow(self, rowIndex):
        """
        :type rowIndex: int
        :rtype: List[int]
        """
        tri = [1]
        if rowIndex == 0:
            return tri
        for i in range(1, rowIndex+1):
            tri = list(map(lambda x, y: x+y, [0]+tri, tri+[0]))
        return tri
